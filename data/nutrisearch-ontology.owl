        <fat>0.4</fat>
        <fiber>2.6</fiber>
        <sustainabilityScore>9.0</sustainabilityScore>
        <season>hiver</season>
        <origin>France</origin>
    </Food>

    <Food rdf:about="http://www.semanticweb.org/nutrisearch-ontology#Carrot">
