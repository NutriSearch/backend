    <!-- NUTRIMENTS -->
    <Nutrient rdf:about="http://www.semanticweb.org/nutrisearch-ontology#VitaminC">
        <rdfs:label>Vitamine C</rdfs:label>
        <dailyRecommendation>80</dailyRecommendation>
        <unit>mg</unit>
    </Nutrient>

    <Nutrient rdf:about="http://www.semanticweb.org/nutrisearch-ontology#Omega3">
        <rdfs:label>Omega-3</rdfs:label>
        <dailyRecommendation>250</dailyRecommendation>
        <unit>mg</unit>
    </Nutrient>

    <Nutrient rdf:about="http://www.semanticweb.org/nutrisearch-ontology#Fiber">
        <rdfs:label>Fibres Alimentaires</rdfs:label>
        <dailyRecommendation>25</dailyRecommendation>
        <unit>g</unit>
    </Nutrient>

    <Nutrient rdf:about="http://www.semanticweb.org/nutrisearch-ontology#Calcium">
        <rdfs:label>Calcium</rdfs:label>
        <dailyRecommendation>1000</dailyRecommendation>
        <unit>mg</unit>
    </Nutrient>

    <Nutrient rdf:about="http://www.semanticweb.org/nutrisearch-ontology#Protein">
        <rdfs:label>Protéines</rdfs:label>
        <dailyRecommendation>50</dailyRecommendation>
        <unit>g</unit>
    </Nutrient>

    <Nutrient rdf:about="http://www.semanticweb.org/nutrisearch-ontology#BetaCarotene">
        <rdfs:label>Bêta-carotène</rdfs:label>
        <dailyRecommendation>800</dailyRecommendation>
        <unit>µg</unit>
    </Nutrient>

    <Nutrient rdf:about="http://www.semanticweb.org/nutrisearch-ontology#Iron">
        <rdfs:label>Fer</rdfs:label>
        <dailyRecommendation>14</dailyRecommendation>
        <unit>mg</unit>
    </Nutrient>

    <!-- EFFETS SANTÉ -->
    <HealthEffect rdf:about="http://www.semanticweb.org/nutrisearch-ontology#ImmuneSupport">
        <rdfs:label>Soutien Immunitaire</rdfs:label>
        <priority>haute</priority>
    </HealthEffect>

    <HealthEffect rdf:about="http://www.semanticweb.org/nutrisearch-ontology#AntiInflammatory">
        <rdfs:label>Anti-inflammatoire</rdfs:label>
        <priority>haute</priority>
    </HealthEffect>

    <HealthEffect rdf:about="http://www.semanticweb.org/nutrisearch-ontology#Digestive">
        <rdfs:label>Améliore la digestion</rdfs:label>
        <priority>moyenne</priority>
    </HealthEffect>

    <HealthEffect rdf:about="http://www.semanticweb.org/nutrisearch-ontology#HeartHealth">
        <rdfs:label>Santé cardiovasculaire</rdfs:label>
        <priority>haute</priority>
    </HealthEffect>

    <HealthEffect rdf:about="http://www.semanticweb.org/nutrisearch-ontology#BrainHealth">
        <rdfs:label>Santé cognitive</rdfs:label>
        <priority>haute</priority>
    </HealthEffect>

    <HealthEffect rdf:about="http://www.semanticweb.org/nutrisearch-ontology#Antioxidant">
        <rdfs:label>Antioxydant</rdfs:label>
        <priority>moyenne</priority>
    </HealthEffect>

    <HealthEffect rdf:about="http://www.semanticweb.org/nutrisearch-ontology#BoneHealth">
        <rdfs:label>Santé osseuse</rdfs:label>
        <priority>moyenne</priority>
    </HealthEffect>

    <!-- RELATIONS -->
